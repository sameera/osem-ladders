{
  "name": "@osem-ladders/cognito-post-signup-lambda",
  "version": "1.0.0",
  "description": "AWS Lambda function for Cognito Post-Signup user provisioning",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist coverage",
    "package": "npm run build && cd dist && zip -r ../function.zip ."
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/node": "^18.19.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.0.4",
    "aws-sdk-client-mock": "^4.1.0",
    "eslint": "^8.56.0",
    "typescript": "~5.5.3",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "aws-lambda",
    "cognito",
    "dynamodb",
    "user-provisioning",
    "serverless"
  ],
  "author": "",
  "license": "ISC"
}
